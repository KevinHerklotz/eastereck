<div class="row p-b-3">
    {% for eastereggname in eastereggnames %}
        <div class="col-xs-12 col-sm-6 col-md-4 col-lg-3 m-y-1">
            <h2>{{ eastereggname }}</h2>
            <button class="btn btn-success" onclick="eastereck.{{ eastereggname }}.start()">
                <i class="fa fa-play"></i> start
            </button>
            <button class="btn btn-danger" onclick="eastereck.{{ eastereggname }}.stop()">
                <i class="fa fa-stop"></i> stop
            </button>
        </div>
    {% endfor %}
</div>
